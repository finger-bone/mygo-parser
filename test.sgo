func aaaa(a int) int {
    var b int;
    b = a * 2;
    return b;
}

decl fib int {}

func fib(n int) int {
    if(n < 2) {
        return n;
    } else {
        return fib(n - 1) + fib(n - 2);
    };
    unreachable;
}

func main() void {
    echo aaaa(2);
    a := 1;
    while(a < 40) {
        echo a;
        echo fib(a);
        a = a + 1;
    };
    return nil;
}