decl fib int {}

func fib(n int, cache int) int {
    if(*(cache + (n * 4)) != 0) {
        return *(cache + (n * 4));
    };
    if(n < 2) {
        cache + (n * 4) <- n;
        return n;
    } else {
        result := fib(n - 1, cache) + fib(n - 2, cache);
        cache + (n * 4) <- result;
        echo n;
        echo result;
        return result;
    };
    unreachable;
}

func main() void {
    a := 1;
    cache malloc 1024 * 64;
    echo fib(40, cache);
    free cache;
    return nil;
}